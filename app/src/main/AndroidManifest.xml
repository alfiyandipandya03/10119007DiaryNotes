<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alfiyandipandya.a10119007_diarynotes"
    xmlns:dist="http://schemas.android.com/apk/distribution">

    <dist:module dist:instant="true" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_launcher_note"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_note"
        android:supportsRtl="true"
        android:theme="@style/Theme.10119007DiaryNotes"
        tools:targetApi="31">
        <activity
            android:name=".activity.diary.AddDiaryActivity"
            android:exported="false"
            android:theme="@style/Theme.10119007DiaryNotes" />
        <activity
            android:name=".activity.diary.EditDiaryActivity"
            android:exported="false"
            android:theme="@style/Theme.10119007DiaryNotes" />
        <activity
            android:name=".activity.diary.DetailDiaryActivity"
            android:exported="false"
            android:theme="@style/Theme.10119007DiaryNotes" />
        <activity
            android:name=".activity.SignUpActivity"
            android:exported="false"
            android:theme="@style/Theme.10119007DiaryNotes" />
        <activity
            android:name=".activity.SignInActivity"
            android:exported="false"
            android:theme="@style/Theme.10119007DiaryNotes" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.10119007DiaryNotes" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity
        android:name=".MainActivity"
        android:exported="false"
        android:theme="@style/Theme.10119007DiaryNotes" />
    </application>

</manifest>